<?xml version="1.0" encoding="utf-8"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <system.webServer>
    <handlers>
      <add name="aspNetCore" path="*" verb="*" modules="AspNetCoreModuleV2" resourceType="Unspecified" xdt:Transform="SetAttributes" xdt:Locator="Match(name)" />
    </handlers>

    <aspNetCore processPath="dotnet" arguments=".\QuizTowerPlatform.Api.dll" stdoutLogEnabled="false" stdoutLogFile=".\logs\stdout" hostingModel="inprocess" xdt:Transform="SetAttributes" />
  </system.webServer>

  <location path="." inheritInChildApplications="false" xdt:Transform="SetAttributes" />
</configuration>
