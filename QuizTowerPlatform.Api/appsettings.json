{
  "Application": {
    "Id": 2,
    "Name": "TowerOfQuizzesPlatform",
    "DisplayName": "Tower of Quizzes",
    "IdPAuthority": "https://localhost:44300",
    "IdPAudience": "towerofquizzesapi",
    "IdPAudienceSecret": "apisecret",
    "OidcClientId": "towerofquizzesplatform",
    "OidcClientSecret": "secret",
    "BackendBaseAddress": "https://localhost:44355",
    "ClientHost": "https://localhost:44365",
    "InfrastructureApiBaseUrl": "https://localhost:44300"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=localhost\\DEVSQLSERVER2019;Initial Catalog=QuizTowerSandbox_Web;Application Name=QuizTowerSandbox_Web;MultipleActiveResultSets=true;Max Pool Size=250;Pooling=True;Integrated Security=True;Encrypt=False;Trust Server Certificate=True"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Debug" },
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "C:\\Logs\\QuizTowerPlatform.Api\\QuizTowerPlatform-.Api.log",
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff} {Level:u3} [{SourceContext}] {Message:lj}{NewLine}{Exception}",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 21,
          "fileSizeLimitBytes": 50000000,
          "rollOnFileSizeLimit": true
        }
      }
    ],
    "Enrich": [ "FromLogContext" ]
  },
  "AllowedHosts": "*",
  "CORS_Url": "http://localhost:3000",
  "EnableSwagger": "true",
  "CustomRemoteCmsApi": {
    "baseUrl": "http://localhost:8000/api/",
    "apiKey": "unknown"
  },
  "EmailRenderer": {
    "SubscribeUrl": "http://localhost:3000/dashboard/towerofquizzes/{0}?page=quizzes"
  }
}